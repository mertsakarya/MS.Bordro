/*  Colors
    Bgrnd Grey: #DADAD2
    Dark Grey:  #B8B8AC #BABAAD
    Light Grey: #CFCEC9
    font:       #474747
    Red:        #CD2024
    Brown:      #80624b
*/

@font-face {
    font-family:'FunctionProBook';
	src: url('FunctionPro-Book-webfont.eot');
	src: url('FunctionPro-Book-webfont.eot?iefix') format('eot'),
         url('FunctionPro-Book-webfont.woff') format('woff'), 
		 url('FunctionPro-Book-webfont.ttf') format('truetype'), 
	     url('functionpro-book-webfont.svg#FunctionProBook') format('svg');             
    font-weight: normal;
	font-style: normal;
}
@font-face {
    font-family: 'FunctionProDemi';
    src: url('FunctionPro-Demi-webfont.eot');
    src: url('FunctionPro-Demi-webfont.eot?iefix') format('eot'),
         url('FunctionPro-Demi-webfont.woff') format('woff'),
         url('FunctionPro-Demi-webfont.ttf') format('truetype'),
         url('FunctionPro-Demi-webfont.svg#webfontalFyZ4NE') format('svg');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'FunctionProMedium';
    src: url('FunctionPro-Medium-webfont.eot');
    src: url('FunctionPro-Medium-webfont.eot?iefix') format('eot'),
         url('FunctionPro-Medium-webfont.woff') format('woff'),
         url('FunctionPro-Medium-webfont.ttf') format('truetype'),
         url('FunctionPro-Medium-webfont.svg#FunctionProMedium') format('svg');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'FunctionProBookOblique';
    src: url('FunctionPro-BookOblique-webfont.eot');
    src: url('FunctionPro-BookOblique-webfont.eot?iefix') format('eot'),
         url('FunctionPro-BookOblique-webfont.woff') format('woff'),
         url('FunctionPro-BookOblique-webfont.ttf') format('truetype'),
         url('FunctionPro-BookOblique-webfont.svg#FunctionProBookOblique') format('svg');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'FunctionProMediumOblique';
    src: url('FunctionPro-MediumOblique-webfont.eot');
    src: url('FunctionPro-MediumOblique-webfont.eot?iefix') format('eot'),
         url('FunctionPro-MediumOblique-webfont.woff') format('woff'),
         url('FunctionPro-MediumOblique-webfont.ttf') format('truetype'),
         url('FunctionPro-MediumOblique-webfont.svg#FunctionProMediumOblique') format('svg');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'NewCenturySchoolbook';
    src: url('NewCenturySchoolbook.ttf');
    src: url('NewCenturySchoolbook.eot?iefix') format('eot'),
         url('NewCenturySchoolbook.woff') format('woff'),
         url('NewCenturySchoolbook.ttf') format('truetype'),
         url('NewCenturySchoolbook.svg#NewCenturySchoolbook') format('svg');
    font-weight: normal;
    font-style: normal;
}

html {
    -webkit-overflow-scrolling: touch;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    -webkit-text-size-adjust: 100%;
    -ms-text-size-adjust: 100%;
}
body {
    -webkit-font-smoothing: antialiased;
    overflow-x: hidden;
    background: #dadad2;
    margin: 0;
    padding: 0;
    font: normal 71.4%/1.4 "FunctionProBook", 'Courier', sans-serif;
    color: #474747;
}
body.hidden {
    overflow: hidden;
}
a {
    outline: 0;
}
img {
    border: 0;
}
.min-margin {
    margin: 82px 0 100px 0;
}
input {
    border-radius: 0;
    -moz-border-radius: 0;
    -webkit-border-radius: 0;
}
h1, h2, h3, h4, h5, h6 {
    font-weight:normal;
}
#title {
    margin-top: 0;
}
header {
    width: 900px;
    height: 189px;
    margin: 20px auto 0 auto;
    text-align: center;
    background: #dadad2;
}
#header-logo {
    width: 232px;
    height: 144px;
    margin: 0 auto 20px auto;
}
.center {
    width: 900px;
    margin: 0 auto;
    font-size: 0.85em;
}
.center h1 {
    margin: 0;
    text-align: center;
    font-size: 2.3em;
    font-family: 'FunctionProDemi';
    text-transform: uppercase;
}

/* cart */

#my-goods.inactive {
    cursor: default;
}

#cart-container {
    width: 900px;
    margin: 0 auto;
}
#cart {
    width: 300px;
    height: 0;
    position: relative;
    margin: 0 auto;
    overflow: hidden;
    z-index: 3;
    border-top: 5px solid #CFCEC9;
}
#cart.active {
    cursor: pointer;
}
#cart-body {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 100px;
    left: 0;
    background: #b8b8ac;
}
#cart-arrow {
    position: absolute;
    height: 100px;
    right: 0;
    bottom: 0;
    left: 0;
    background: url('cart-arrow.png') no-repeat;
}
#cart section {
    margin: 0 20px;
    width: 260px;
}
#cart footer {
    width: 300px;
    text-align: center;
}
#cart footer a {
    text-transform: uppercase;
    text-decoration: none;
    color: #474747;
    font: 1.2em/0 'FunctionProDemi', Courier;
}
#cart footer a:hover {
    color: #dadad2;
}
#cart section, #cart footer {
    opacity: 0;
}
#cart section article {
    width: 260px;
    height: 100px;
    position: relative;
    margin-top: 10px;
}
#cart section article img {
    width: 100px;
    height: 100px;
    position: absolute;
    top: 0;
    left: 80px;
}
#cart section article > a {
    width: 10px;
    height: 10px;
    position: absolute;
    top: 48px;
    left: 18px;
    font-size: 24px;
    color: #474747;
    line-height: 10px;
    text-decoration: none;
}
#cart section article > a:hover {
    color: #cd2024;
}
#cart section article .quantity {
    position: absolute;
    top: 38px;
    left: 46px;
    margin: 0;
    font-size: 1.75em;
    color: #dadad2;
    font-family: 'FunctionProMediumOblique';
}
#cart section article .price {
    position: absolute;
    top: 34px;
    right: 0;
    margin: 0;
    text-align: right;
    font: 1.5em/2.1em 'FunctionProDemi';
}
#cart section article .price .cur {
    margin-right: 5px;
    font: .6em 'FunctionProBook', Courier, 'Courier New';
}
#cart section footer {
    width: 260px;
}
#cart section footer.active {
    margin-top: 5px;
    border-top: 5px solid #dadad2;
}
#cart section footer div {
    margin: 10px auto;
    color: #474747;
    text-transform: uppercase;
    font-family: 'FunctionProDemi';
}
#cart section footer div .total {
    font-size: 1.3em;
}
#cart section footer div .cur {
    margin: 0 5px;
    font-size: 0.9em;
}
#cart section footer div .sum {
    font-size: 1.5em;
}
#cart section footer div:first-child {
    margin: 10px 0 0 0;
}
#cart section footer .taxes {
    padding-bottom: 10px;
    margin: 0 0 10px 0;
    text-transform: none;
    border-bottom: 1px solid #dadad2;
    font: 1.1em 'FunctionProBook', Courier, 'Courier New';
}
body > section, body > footer {
    position: relative;
    margin: 0 auto;
    width: 900px;
    text-align: center;
}
body > section {
    font-size: 0.85em;
}
body > section h1 {
    font-size: 1.8em;
    font-family: 'NewCenturyschoolbook', FunctionProBook;
    text-transform: none;
}
body > section p {
    font-size: 1.6em;
}
body > footer {
    padding: 0 0 1em;
    margin: 0 auto;
    width: 900px;
}
hr {
    width: 280px;
    height: 5px;
    clear: both;
    margin: 20px auto;
    border: none;
    background: #CFCEC9;
}
footer hr {
    width: 110px;
}
footer ul {
    padding: 0;
    margin: 0;
    list-style: none;
}
#menu {
    font-family: 'FunctionProBook', Courier, "Courier New", mono;
    text-transform:uppercase;
    font-size: 1.1em;
}
#menu h2 {
    font-size: 1.3em;
}
#ribbon {
    width: auto;
    height: 26px;
    margin: 10px 0 20px 0;
    background: url('ribbon.gif?1') no-repeat center center;
}

#weare {
    font-family: 'NewCenturySchoolbook', 'FunctionProBook', "Courier New", mono;
    text-transform:none;
    font-size: 1em;
    margin-bottom: 10px;
}

#weare a {
   color: #474747;
}

#menu a {
    text-decoration: none;
    color: #474747;
}
#menu a:hover {
    color: #BABAAD;
}

/* Social */

#social {
    width: 110px;
    overflow: auto;
    margin: 0 auto;
    margin-top: 10px;
    margin-bottom: 20px;

}
#social li {
    width: 20px;
    height: 20px;
    position: relative;
    float: left;
    margin-right: 10px;
    background-repeat: no-repeat;
    overflow: hidden;
}
#social li:last-child {
    margin: 0;
}
#social li:hover {
    background-position: -20px center;
}
#social a {
    width: 20px;
    height: 20px;
    position: absolute;
    top: 0;
    left: 0;
}
#social .instagram {
    background-image: url('instagram.png?1');
}
#social .twitter {
    background-image: url('twitter.png?1');
}
#social .twitter iframe {
    opacity: 0;
}
#social .fancy {
    background-image: url('fancy.png?1');
}
#social .tumblr {
    background-image: url('tumblr.png?1');
}

footer section {margin:2em 0;}
footer section h1 {font:1em 'FunctionProDemi'; text-transform:uppercase;}

/* Shipping & Payment */

#shipping, #payment {
    margin-bottom: 20px;
}
#shipping h1, #payment h1 {
    margin: 0;
    font: 1em 'FunctionProDemi';
    text-transform: uppercase;
}
#shipping h1 {
    margin-bottom: 10px;
}
#shipping p, #payment p {
    margin: 0;
}

footer > span {color:#BABAAD;}
footer #footer-mainmenulink {font-family:'FunctionProDemi';}

.product h1 {
    margin-top: 0;
    font-family: 'NewCenturySchoolbook';
    text-transform: none;
}
.product, .product article {
    width: 875px;
    position: relative;
    margin: 0 auto;
}
.product #visual, .product #details, .product #materials {
    height: 580px;
    background: rgba(218, 218, 210, 0.9);
}
.product #visual {
    cursor: pointer;
}
.product #visual.in-active {
    cursor: default;
}
.product #details, .product #materials {
    width: 875px;
    height: 580px;
    position: absolute;
    top: 0;
    left: 0;
    cursor: pointer;
}
#image-materials {
    position: absolute;
    top: 0;
    left: 0;
    opacity: 0;
}
.center-wrapper {
    width: 550px;
    margin: 0 auto;
}
.product article ul {text-align:left; font-size:1.5em; margin:1em; list-style:none; text-indent:0; padding:0;}
.product article ul li {margin-bottom:0.3em;}
.product article p {
	font: 1.3em/1.3em 'NewCenturySchoolbook', FunctionProBook;
}
.product #details h1,
.product #materials h1 {display:inline-block; font:1.8em/0.8 'FunctionProBook'; margin:1em 0 0.5em 0; text-transform: uppercase;}

.product #visual a {
    width: 875px;
    height: 580px;
    display: block;
}
#details, #materials {
    visibility: hidden;
}
.product #details a, #materials a {text-decoration:underline; color:inherit;}
.product #details a:hover, #materials a:hover {color:#fff;}
.product #details-show-images {font-family: 'FunctionProBook'; text-transform: uppercase; text-decoration:none;
	font-weight: normal;
	font-style: normal;
	margin-top: 10px;
}

.product #materials p {margin:0 1.5em 0.5em;}

.product article footer {
    margin-top: 20px;
}
.product article footer ul {
    width: 280px;
    height: 22px;
    margin: 0 auto 20px auto;
    padding: 0;
    border-bottom: 5px solid #CFCEC9;
}
.product article footer li {
    width: 90px;
    float: left;
    margin: 0 5px 0 0;
    font-size: 0.85em;
    text-align: center;
}
.product article footer li.in-active {
    opacity: 0.5;
}
.product article footer li:last-child {
    margin: 0;
}
.product article footer li a {
    display: block;
    width: 90px;
    padding: 4px 0 2px 0;
    color: #474747;
    text-decoration: none;
    text-transform: uppercase;
    font-family: 'FunctionProDemi';
    background: #B8B8AC;
}
.product article footer li a:hover, .product article footer li a.active {
    background: #474747;
    color: #DADAD2;
}
.product article footer li.in-active a:hover {
    color: #474747;
    background: #B8B8AC;
    cursor: default;
}
.product aside {
    position: absolute;
    top: 0;
    right: 0;
    width: 200px;
}
#variants {
    margin: 0 0 20px 0;
    list-style: none;
    float: right;
    width: 200px;
}
#variants li {
    width: 250px;
    margin: 0 0 5px 85px;
}
#variants li:last-child {
    margin-bottom: 0;
}
#variants label {
    font-size: 0.9em;
    cursor: pointer;
}
#options input[type=radio] {
    width: 14px;
    height: 14px;
    padding: 0;
    margin: -1px 5px 0 0;
    vertical-align: text-top;
    border: 1px solid #474747;
    border-radius: 7px;
    -moz-border-radius: 7px;
    -webkit-border-radius: 7px;
    -webkit-appearance: none;
}
#options input[type=radio]:hover, .product #options input[type=radio]:checked {
    background: url('bgrnd-radiobtn4.png') center center no-repeat;
}

/* buy */

#buy {
    width: 115px;
    height: 57px;
    position: relative;
    float: right;
    margin-bottom: 8px;
    cursor: pointer;
}
#buy p {
    color: #474747;
    text-transform: uppercase;
    font-family: 'FunctionProDemi';
}
#buy .cur {
    font-size: 0.75em;
    font-family: 'FunctionProBook';
}
#buy .box {
    width: 115px;
    height: 22px;
    position: absolute;
    bottom: 0;
    left: 0;
    padding-top: 5px;
    background: #b8b8ac;
}
#buy .box.active {
    background: #474747;
}
#buy .done {
    margin: 0;
    color: #fff;
    opacity: 0;
    display: none;
}
#buy .label {
    width: 115px;
    position: absolute;
    top: 22px;
    left: 0;
    transition: all 0.25s ease;
    -o-transition: all 0.25s ease;
    -moz-transition: all 0.25s ease;
    -webkit-transition: all 0.25s ease;
}
#buy:hover .label {
    top: 34px;
}
#buy .arrow {
    width: 7px;
    height: 16px;
    position: absolute;
    top: -23px;
    left: 54px;
    background: url('buy-arrow.png') no-repeat;
}
#buy .price {
    margin: 0;
}
#options .small-print {
    width: 115px;
    float: right;
    margin: 0;
    color: #474747;
    font-size: 0.9em;
    text-transform: uppercase;
    font-family: 'FunctionProBook';
}
#options .error {
    width: 115px;
    float: right;
    margin: 20px 0;
    opacity: 0;
    color: #ca2027;
    font-size: 0.65em;
    text-transform: uppercase;
}
.hide {
    display: none;
}

/* out of stock */

#out-of-stock {
    width: 145px;
    padding: 10px;
    margin-bottom: 20px;
    float: right;
    background: #cfcec9;
}
#out-of-stock h1 {
    margin: 0 0 10px 0;
    font-size: 1.65em;
}
#out-of-stock h2 {
    margin: 0;
    font-size: 1.25em;
    font-family: 'FunctionProBookOblique';
}
#out-of-stock .description {
    margin-bottom: 10px;
}
#out-of-stock form {
    width: 145px;
    height: 30px;
    margin-bottom: 10px;
}
#out-of-stock input[type=text] {
    width: 95px;
    padding: 10px 10px 7px 10px;
    margin: 0;
    float: left;
    border: none;
    outline: 0;
    color: #474747;
    font-family: 'FunctionProBook';
    text-transform: uppercase;
}
#out-of-stock input[type=submit] {
    width: 30px;
    height: 30px;
    padding: 0;
    margin: 0;
    float: right;
    border: none;
    outline: 0;
    background: url('submit-small.gif');
}

/* share */

#share {
    width: 55px;
    position: relative;
    padding-left: 10px;
    float: right;
    cursor: pointer;
    opacity: 0;
    display: none;
}
#share ul {
    width: auto;
    height: 20px;
    padding: 0;
    margin: 0;
    list-style: none;
    opacity: 0;
    display: none;
}
#share li {
    padding: 0;
    margin: 0 20px 0 0;
    float: left;
}
#share li:first-child {
    margin: 0 19px 0 3px;
    line-height: 25px;
    font-size: 1.5em;
}
#share li:last-child {
    margin: 0;
}
#share p {
    padding: 1px 0 0 0;
    margin: 0;
    text-transform: uppercase;
    font-family: 'FunctionProDemi';
}
#share-arrow {
    width: 10px;
    height: 40px;
    position: absolute;
    top: 0;
    left: 0;
    background: url('share-arrow.png?2') no-repeat;
}
#share-content {
    height: 20px;
    padding: 10px 10px 10px 5px;
    background: #cfcec9;
}
#share #twitter iframe {
    width: 81px !important;
}
#share #plus-one iframe, #share #___plusone_0 {
    width: 59px !important;
}
#share #pinterest {
    height: 24px;
    margin-top: -2px;
}
#share #pinterest iframe {
